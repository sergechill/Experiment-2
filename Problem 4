#include <stdio.h>

int main()
{
	int x, y, z, largest;
	
	printf("Enter first number: ");
	scanf("%d",&x);
	printf("Enter second number: ");
	scanf("%d",&y);
	printf("Enter third number: ");
	scanf("%d",&z);	
	
	if(x > y && x > z)
		largest = x;
	
	if(y > x && y > z)
		largest = y;
		
	if(z > x && z > y)
		largest = z;
		
	printf("The LARGEST of the three numbers is %d.",largest);
	return 0;
}
